<div class="container">
  <div class="row">
    <div class="col"></div>
      <div class="col">
        <h1>Login</h1>        
        <%= form_tag "/login" do |f| %>
        Email
        <br>
        <%= text_field_tag :email %> 
        <br>
        Password
        <br>
        <%= password_field_tag :password %>
        <br>
        <%= submit_tag "Submit" %>
        <% end %>
      </div>
    <div class="col"></div>
  </div>
</div>


